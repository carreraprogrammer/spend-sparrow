<p style="color: green"><%= notice %></p>
<div class="general-container">
  <div id="categories">
    <% @categories.each do |category| %>
      <%= link_to category do %>
        <%= render category %>
      <% end %>
    <% end %>
  </div>
</div>

<%= link_to "New category", new_category_path, class: "new-category-button" %>